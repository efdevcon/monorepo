{
  "name": "devcon-api",
  "description": "Devcon API exposes information about Devcon, past events, schedules and our video archive. You can find more information at https://github.com/efdevcon/api",
  "homepage": "https://api.devcon.org",
  "author": "support@devcon.org",
  "version": "0.6.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/efdevcon/api"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "NODE_PATH=./dist node ./dist/devcon-api/src/index",
    "dev:generate": "yarn swagger:generate && yarn db:generate && nodemon ./src/index.ts",
    "dev": "nodemon ./src/index.ts",
    "test": "NODE_ENV=test jest --detectOpenHandles --watchAll --no-cache",
    "test:coverage": "NODE_ENV=test jest --coverage --detectOpenHandles --no-cache",
    "lint": "eslint --fix",
    "prettier": "prettier './src' --write",
    "migrate": "ts-node ./src/scripts/migrate.ts",
    "slides": "ts-node ./src/scripts/slides.ts",
    "stats": "ts-node ./src/scripts/stats.ts",
    "stats:v": "ts-node ./src/scripts/stats-video.ts",
    "usernames": "ts-node ./src/scripts/usernames.ts",
    "sync:pretalx": "ts-node ./src/scripts/sync-pretalx.ts",
    "db:generate:all": "prisma generate --schema src/db/schema.prisma && prisma generate --schema src/db/account.prisma && yarn copy:account",
    "db:generate": "yarn db:generate:all && prisma db push --schema src/db/schema.prisma --force-reset --accept-data-loss && prisma db push --schema src/db/account.prisma && prisma db seed",
    "copy:account": "mkdir -p dist/db && cp src/db/clients dist/db/clients -r",
    "db:seed:account": "prisma db push --force-reset --accept-data-loss --schema src/db/account.prisma && ts-node src/db/account-seed",
    "db:studio": "prisma studio",
    "db:studio:account": "prisma studio --schema src/db/account.prisma",
    "swagger:generate": "ts-node ./src/swagger/generate",
    "postinstall": "yarn swagger:generate && yarn db:generate",
    "ai-schedule-sync": "ts-node src/services/ai/open-ai/sync-vector-store"
  },
  "prisma": {
    "schema": "./src/db/schema.prisma",
    "seed": "tsx ./src/db/seed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src": [
      "lint",
      "prettier"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "./src",
    "coverageDirectory": "../coverage",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  },
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "@parcnet-js/ticket-spec": "^1.1.8",
    "@pcd/pod": "^0.4.0",
    "@prisma/client": "^5.21.1",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^6.12.1",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "googleapis": "^144.0.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "jose": "^5.9.6",
    "llamaindex": "^0.3.10",
    "markdown-it": "^13.0.1",
    "natural": "^7.1.0",
    "nodemailer": "^6.9.13",
    "openai": "^4.55.5",
    "pg": "^8.13.1",
    "puppeteer": "18.2.1",
    "rate-limiter-flexible": "^5.0.3",
    "remark": "^15.0.1",
    "remark-mdx": "^3.0.1",
    "remark-mdx-to-plain-text": "^3.0.0",
    "rss-parser": "^3.13.0",
    "slugify": "^1.6.5",
    "swagger-ui-express": "^4.6.0",
    "tsconfig-paths": "^4.1.2",
    "unique-names-generator": "^4.7.1",
    "viem": "^2.21.40",
    "web-push": "^3.6.7",
    "yaml": "^2.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@octokit/types": "^9.0.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.18.0",
    "@types/handlebars": "^4.1.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.2.5",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.15",
    "@types/puppeteer": "^7.0.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prisma": "^5.21.1",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.22.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
