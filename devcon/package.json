    "@tippyjs/react": "^4.2.6",
    "@web3modal/wagmi": "^5.0.5",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^15.0.0",
    "ethers": "^5.5.4",
    "framer-motion": "^10.16.4",
    "gray-matter": "^4.0.3",
    "gsap": "^3.5.0",
    "jose": "^5.6.3",
    "markdown-it": "^12.3.2",
    "markdown-it-video": "^0.6.3",
    "matter-js": "^0.18.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "next": "13",
    "next-auth": "^4.24.7",
    "next-intl": "^2.3.5",
    "node-fetch": "2",
    "nodemailer": "^6.7.2",
    "react": "^18.2.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-cool-dimensions": "^2.0.7",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.25.3",
