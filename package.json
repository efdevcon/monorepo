{
  "name": "devcon-monorepo",
  "version": "0.0.1",
  "description": "Devcon Monorepo",
  "private": true,
  "scripts": {
    "dev:devconnect": "pnpm --filter devconnect dev",
    "build:devconnect": "pnpm --filter devconnect build",
    "start:devconnect": "pnpm --filter devconnect start",
    "dev:devconnect-app": "pnpm --filter devconnect-app dev",
    "build:devconnect-app": "pnpm --filter devconnect-app build",
    "start:devconnect-app": "pnpm --filter devconnect-app start",
    "dev:archive": "pnpm --filter archive dev",
    "build:archive": "pnpm --filter archive build",
    "start:archive": "pnpm --filter archive start",
    "dev:devcon": "pnpm --filter devcon dev",
    "build:devcon": "pnpm --filter devcon build",
    "start:devcon": "pnpm --filter devcon start",
    "dev:devcon-app": "pnpm --filter devcon-app dev",
    "build:devcon-app": "pnpm --filter devcon-app build",
    "start:devcon-app": "pnpm --filter devcon-app start",
    "dev:devcon-api": "pnpm --filter devcon-api dev",
    "build:devcon-api": "pnpm --filter devcon-api build",
    "start:devcon-api": "pnpm --filter devcon-api start"
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
    "overrides": {
      "devconnect>@reown/appkit": "1.7.17",
      "devconnect>@reown/appkit-adapter-wagmi": "1.7.17"
    }
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
